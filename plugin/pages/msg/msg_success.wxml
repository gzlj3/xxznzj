<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">{{button2Obj?'操作完成':'存盘完成'}}</view>
            <view class="weui-msg__desc addfontsize18">{{pageDesc}}</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" bindtap="onButton1">返回</button>
                <button class="weui-btn" type="default" bindtap="onButton2" wx:if="{{button2Obj}}">{{button2Obj.buttonText}}</button>
                <!-- <button class="weui-btn" type="default" bindtap="onButton3" wx:else>返回列表</button> -->
            </view>
        </view>
        <!-- <view class="weui-msg__extra-area">
            <view class="weui-footer">
